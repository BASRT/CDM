@model IEnumerable<ConcremoteDeviceManagment.Models.Pricelist>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.id_cat)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.id_subcat)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.price)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.art_lev_nr)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.id_cat)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.id_subcat)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.price)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.art_lev_nr)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Price_id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Price_id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Price_id })
        </td>
    </tr>
}

</table>
